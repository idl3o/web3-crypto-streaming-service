{
  "name": "@web3-crypto-streaming/service",
  "version": "1.0.0",
  "description": "Web3 Crypto Streaming Platform with advanced features",
  "main": "server.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run build && node server.js",
    "serve": "node server.js",
    "test": "jest",
    "launch": "node launcher.js",
    "webserv": "npm run build && node launcher.js",
    "vscode-server": "node vscode-server.js",
    "vscode-server:install": "npm install code-server open",
    "prepare": "npm run build",
    "codespace:dev": "GITHUB_CODESPACES=true npm run dev",
    "codespace:test": "GITHUB_CODESPACES=true npm test",
    "ai:gemini": "node scripts/ai/test-gemini.js",
    "ai:test-gemini": "node scripts/ai/test-gemini.js",
    "install:cli": "node scripts/external/install-cli.js",
    "cli": "npx web3-cli",
    "cli:update": "npm run install:cli",
    "ios:build": "node scripts/ios/build.js",
    "ios:open": "npx cap open ios",
    "ios:sync": "npx cap sync ios",
    "ios:run": "npx cap run ios",
    "ios:update": "npx cap update ios",
    "init-ios": "npx cap add ios",
    "build:babel": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps",
    "build:prod": "NODE_ENV=production babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps false",
    "publish:npm": "npm publish",
    "publish:dry-run": "npm publish --dry-run",
    "setup:github-registry": "node scripts/setup-github-registry.js",
    "ci:fix-deps": "node scripts/ci/fix-dependencies.js",
    "postinstall": "node -e \"console.log('\\nSetup completed! If you encounter any issues, run: npm run ci:fix-deps\\n')\"",
    "setup:homepod": "node scripts/setup/homepod-setup.js",
    "stream": "node scripts/streaming/stream.js"
  },
  "keywords": [
    "blockchain",
    "web3",
    "streaming",
    "poe",
    "crypto"
  ],
  "author": "Web3 Crypto Streaming Team",
  "license": "MIT",
